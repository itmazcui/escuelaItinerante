@model EscuelaItinerante.Models.NuevoAlumnoViewModel
@{
    ViewBag.Title = "Alta de Alumno ";
}
<h2>@ViewBag.Title.</h2>

<div class="container">
    <h3 class="text-success">Exito!</h3>
    <p>El alumno ha sido de alta correctamente.</p>
    <hr />
    <h3>Seleccione que operación desea realizar:</h3>
    <div class="row mt20">
        <div class="col-md-6 text-center">
            <a href="/" class="btn btn-info w230">Inicio</a>
        </div>
        <div class="col-md-6 text-center">
            <a href="@Url.Action("InscribirAlumno", "Alumno" )" class="btn btn-info w230">Inscribir Alumno</a>
        </div>
        <div class="col-md-6 text-center mt20">
            <a href="@Url.Action("NuevoPago", "Pago" )" class="btn btn-info w230">Nuevo Pago</a>
        </div>
        <div class="col-md-6 text-center mt20">
            <a href="@Url.Action("AlumnoAbandonaComision", "Alumno" )" class="btn btn-info w230">Baja de Alumno a Curso</a>
        </div>
    </div>
</div>